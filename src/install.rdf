<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <!-- <em:aboutURL>chrome://mailfrom/content/about.xul</em:aboutURL> -->
        <em:creator>Gareth Hunt</em:creator>
        <em:description>Enables mailto links to point to any webmail service for writing email.</em:description>
        <em:homepageURL>http://www.garethhunt.com/</em:homepageURL>
        <!-- <em:iconURL>chrome://mailfrom/content/logo.gif</em:iconURL> -->
        <em:iconURL></em:iconURL>
        <!-- UUID for the UI prototype -->
        <em:id>@build.mozilla.firefox.extension.uuid@</em:id>
        <em:name>MailFrom</em:name>
        <em:optionsURL>chrome://mailfrom/content/preferences.xul</em:optionsURL>
        <em:version>@build.version@</em:version>
        <em:file>
            <Description about="urn:mozilla:extension:file:mailfrom.jar">
                <!-- <em:locale>locale/en-US/</em:locale> -->
                <em:package>content/</em:package>
            </Description>
        </em:file>
        <!-- Firefox 1.* - 3.0.* -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>1.0</em:minVersion>
                <em:maxVersion>3.0.*</em:maxVersion>
            </Description>
        </em:targetApplication>
    </Description>
</RDF>
